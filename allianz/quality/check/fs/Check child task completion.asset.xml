  <asset_feature feature="censhare:completeness-check.expression.xpath" value_string="not(child_asset_rel[@key=&apos;user.task.&apos;]) or exists(asset_feature[@feature=&apos;censhare:calc-child-task-completion-percentage&apos; and @value_double=&apos;100.0&apos;])"/>
