  <asset_feature feature="censhare:name" language="de" value_string="Aufgaben erledigt"/>
  <asset_feature feature="censhare:name" language="en" value_string="Tasks completed"/>
  <asset_feature feature="censhare:name" language="fr" value_string="Tâches terminées"/>
